<script> 
function myExampleFunction(){
let myIn01 = parseInt(document.getElementById('myText01').value)
let mySum = myIn01 + myIn01
let myReply = myIn01 + ' + ' + myIn01 + ' = ' + mySum
return myReply 





}



</script>

<script>

function myFunction05(){
let myIn03 = parseInt(document.getElementById('myText03').value)
let myIn04 = parseInt(document.getElementById('myText04').value)
let mySum01 = myIn03 * myIn04
let wiggles = myIn03 * ' x' * myIn04 + ' = ' * mySum01
return wiggles

}


</script>

